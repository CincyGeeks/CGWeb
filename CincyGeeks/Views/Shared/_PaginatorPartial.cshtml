@model CincyGeeksWebsite.Models.Shared.PaginatorPartialModel
<div style="overflow:auto; width:100%;">
    <div style="float:left; margin:5px; padding:5px; border:1px solid black;">
        @if (Model.FirstPageParamerterObject != null)
        {
            @Html.ActionLink("First", Model.ActionName, Model.FirstPageParamerterObject);
        }
        else
        {
            @Html.Label("First");   
        }
    </div>
    <div style="float:left; margin:5px; padding:5px; border:1px solid black;">
        @if (Model.PrevPageParamerterObject != null)
        {
            @Html.ActionLink("Prev", Model.ActionName, Model.PrevPageParamerterObject);
        }
        else
        {
            @Html.Label("Prev");   
        }
    </div>
    @for (int i = 0; i < Model.PageLinkValues.Count(); i++)
    {
        <div style="float:left; margin:5px; padding:5px; border:1px solid black;">
            @if (Model.PageLinkParameterObjects[i] != null)
            {
                @Html.ActionLink(Model.PageLinkValues[i], Model.ActionName, Model.PageLinkParameterObjects[i]);
            }
            else
            {
                @Html.Label(Model.PageLinkValues[i]);
            }
        </div>
    }
    <div style="float:left; margin:5px; padding:5px; border:1px solid black;">
        @if (Model.NextPageParamerterObject != null)
        {
            @Html.ActionLink("Next", Model.ActionName, Model.NextPageParamerterObject);
        }
        else
        {
            @Html.Label("Next");   
        }
    </div>
    <div style="float:left; margin:5px; padding:5px; border:1px solid black;">
        @if (Model.LastPageParamerterObject != null)
        {
            @Html.ActionLink("Last", Model.ActionName, Model.LastPageParamerterObject);
        }
        else
        {
            @Html.Label("Last");   
        }
    </div>
</div>
